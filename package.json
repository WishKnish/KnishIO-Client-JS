{
  "name": "@wishknish/knishio-client-js",
  "version": "0.6.4",
  "productName": "Knish.IO Javascript SDK Client",
  "description": "JavaScript implementation of the Knish.IO SDK to consume Knish.IO GraphQL APIs.",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WishKnish/KnishIO-Client-JS.git"
  },
  "contributors": [
    {
      "name": "Eugene Teplitsky",
      "email": "eugene@wishknish.com",
      "homepage": "https://wishknish.com",
      "role": "developer"
    },
    {
      "name": "Vladimir Makarov",
      "email": "vladimir@wishknish.com",
      "homepage": "https://wishknish.com",
      "role": "developer"
    },
    {
      "name": "Yuri Kizilov",
      "email": "yuriy@wishknish.com",
      "homepage": "https://wishknish.com",
      "role": "developer"
    }
  ],
  "main": "dist/client.iife.js",
  "module": "dist/client.es.js",
  "commonjs": "dist/client.cjs.js",
  "keywords": [
    "wishknish",
    "knishio",
    "blockchain",
    "dag",
    "client",
    "graphql"
  ],
  "dependencies": {
    "@noble/post-quantum": "^0.4.1",
    "@thumbmarkjs/thumbmarkjs": "^0.20.5",
    "@urql/core": "^5.1.1",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.5",
    "isomorphic-fetch": "^3.0.0",
    "jssha": "^3.3.1",
    "wonka": "^6.3.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.34.1",
    "buffer": "^6.0.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-vue": "^9.33.0",
    "esmock": "^2.7.0",
    "jest": "^29.7.0",
    "rollup": "^4.44.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build --config build/vite.config.mjs",
    "build:js": "vite build --config build/vite.config.js.mjs",
    "build:ts": "tsc --noEmit && vite build --config build/vite.config.mjs",
    "lint": "eslint --ext .js,.ts src",
    "lint:fix": "eslint --ext .js,.ts src --fix",
    "lint:js": "eslint --ext .js src",
    "lint:ts": "eslint --ext .ts src",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:js": "jest --config jest.config.js.json --testPathPattern=\"\\.(js)\"",
    "test:ts": "jest --config jest.config.ts.json --testPathPattern=\"\\.(ts)\"",
    "test:interop": "jest __tests__/interchangeability.test.js",
    "test:watch": "jest --watch",
    "test:watch:js": "jest --config jest.config.js.json --watch --testPathPattern=\"\\.(js)\"",
    "test:watch:ts": "jest --config jest.config.ts.json --watch --testPathPattern=\"\\.(ts)\"",
    "dev": "vite build --watch --config build/vite.config.mjs",
    "dev:js": "vite build --watch --config build/vite.config.js.mjs",
    "dev:ts": "vite build --watch --config build/vite.config.mjs",
    "compare": "node scripts/compare-outputs.js",
    "migrate:verify": "node scripts/verify-migration.js"
  },
  "files": [
    "/src",
    "/dist"
  ],
  "types": "dist/index.d.ts"
}
